{
  "name": "vite-template-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && yarn ci && vite build",
    "check-types": "tsc",
    "lint": "yarn check-types && yarn eslint && yarn stylelint",
    "eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "stylelint": "stylelint \"**/*.{css,scss,sass}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "test": "vitest",
    "ci": "yarn lint && yarn test --watch=false",
    "audit-deps": "yarn improved-yarn-audit --ignore-dev-deps --min-severity moderate --retry-on-network-failure",
    "audit:fix": "npx yarn-audit-fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.22.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "improved-yarn-audit": "^3.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "stylelint": "^15.7.0",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^5.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.31.4",
    "yarn-audit-fix": "^9.3.10"
  }
}
